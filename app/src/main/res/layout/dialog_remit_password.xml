            app:layout_constraintStart_toStartOf="@+id/line6"
            app:layout_constraintBottom_toBottomOf="@+id/line6"/>
        <View
            android:id="@+id/line1"
            android:layout_marginStart="-5dp"
            android:background="@color/gray_545454"
            android:layout_width="25dp"
            android:layout_height="1dp"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_remit_password_info"
            app:layout_constraintStart_toStartOf="@+id/tv_remit_password_info"/>
        <View
            android:id="@+id/line2"
            android:background="@color/gray_545454"
            android:layout_width="25dp"
            android:layout_height="1dp"
            android:layout_marginTop="55dp"
            android:layout_marginStart="14dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_remit_password_info"
